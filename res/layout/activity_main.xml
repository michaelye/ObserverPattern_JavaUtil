<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    
    <SeekBar
        android:id="@+id/sb_data_changer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp" />
    
    <com.michael.observerpattern_javautil.ObserverButton 
        android:id="@+id/btn_observer_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sb_data_changer"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        />
    
    <com.michael.observerpattern_javautil.ObserverButton 
        android:id="@+id/btn_observer_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_observer_one"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        />
    
    <com.michael.observerpattern_javautil.ObserverButton 
        android:id="@+id/btn_observer_three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btn_observer_two"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        />
    
    

</RelativeLayout>
